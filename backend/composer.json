{
    "require": {
        "bryanjhv/slim-session": "4.0.0",
        "illuminate/database": "^6.8",
        "php-di/php-di": "^6.0",
        "selective/config": "^0.1.1",
        "slim/http": "^0.8.0",
        "slim/psr7": "^0.6.0",
        "slim/slim": "4.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "3.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\" : "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
